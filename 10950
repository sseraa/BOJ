/*
  #10950 ; A+B-3
  >> 너무 어렵게 생각 nono

*/

import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        
        Scanner sc = new Scanner(System.in);
        
        int num = sc.nextInt();
       
        int A = 0;
        int B = 0;

        //sc.close();

        for(int i=0;i<num;i++) {
          A = sc.nextInt();
          B = sc.nextInt();

          System.out.println(A+B);
        }

        
        

    }
    
}
