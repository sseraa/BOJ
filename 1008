// 1008 A/B

import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        //to include under 0, X Int
        double A = sc.nextDouble();
        double B = sc.nextDouble();
        //sc.close(); 
        
        System.out.println(A / B);
        

    }
    

}
